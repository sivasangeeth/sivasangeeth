import java.util.*;
class fgh{
	public static void main (String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
	    int a=sc.nextInt();
	    int b=sc.nextInt();
	    if(n>a&&n>b)
	    System.out.print(n);
	    else if(a>b&&a>n)
	    System.out.print(a);
	    else
	    System.out.print(b);
}
}
